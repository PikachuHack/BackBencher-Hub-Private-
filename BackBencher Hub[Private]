--Buttons
Tab1Section:NewButton("Buy A", "Buys A once", function()
  local args = {
    [1] = "A"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

end)
Tab1Section:NewToggle("auto buy A", "auto buys A", function(state)
    if state then
        _G.on = true
        while _G.on do
            wait()
            local args = {
                [1] = "A"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
        end
    else
        _G.on = false
    end
end)
